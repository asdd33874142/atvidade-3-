se (éPerfeito(num)) {
    cout << num << "é um número perfeito." << fim;
} outro {
    cout << num << "não é um número perfeito." << fim;
}

retornar 0;
}

PERGUNTA 20: #incluir usando namespace std;

// Função para ordenar o vetor usando o algoritmo Bubble Sort void bubbleSort(int arr[], int n) { for (int i = 0; i < n - 1; ++i) { for (int j = 0; j < n - i - 1; ++j) { if (arr[j] > arr[j + 1]) { // Troca arr[j] e arr[j + 1] int temp = arr[j]; arr[j] = arr[j + 1]; arr[j + 1] = temp; } } } }

// Função para exibir os elementos do vetor void exibirVetor(int arr[], int n) { for (int i = 0; i < n; ++i) { cout << arr[i] << " "; } cout << endl; }

int principal() { int n;

cout << "Digite o número de elementos no vetor: ";
cin >> n;

int* arr = novo int[n]; // Cria um vetor dinamicamente

cout << "Digite os elementos do vetor: ";
para (int i = 0; i < n; ++i) {
    cin >> arr[i];
}

// Ordem ou vetor
bubbleSort(arr, n);

// Exibe o vetor ordenado
cout << "Vetor ordenado em ordem crescente: ";
exibirVetor(arr, n);

excluir[] arr; // Libera a memória alocada para o vetor

retornar 0;
}

PERGUNTA 21: #incluir usando namespace std;

// Função para realizar a busca linear em um vetor int buscaLinear(const int arr[], int n, int valor) { for (int i = 0; i < n; ++i) { if (arr[i] = = valor) { retornar i; // Retorna o índice onde o valor foi encontrado } } return -1; // Retorna -1 se o valor não for encontrado }

int main() { int n, valor;

cout << "Digite o número de elementos no vetor: ";
cin >> n;

int* arr = novo int[n]; // Cria um vetor dinamicamente

cout << "Digite os elementos do vetor: ";
para (int i = 0; i < n; ++i) {
    cin >> arr[i];
}

cout << "Digite o valor a ser buscado: ";
cin >> valor;

int resultado = buscaLinear(arr, n, valor);

se (resultado != -1) {
    cout << "Valor " << valor << " encontrado no índice " << resultado << "." << fim;
} outro {
    cout << "Valor " << valor << " não encontrado no vetor." << fim;
}

excluir[] arr; // Libera a memória alocada para o vetor

retornar 0;
}

PERGUNTA 22: #incluir usando namespace std;

// Função para realizar a busca binária em um vetor ordenado int buscaBinaria(const int arr[], int n, int valor) { int esquerda = 0; int direita=n - 1;

enquanto (esquerda <= direita) {
    int meio = esquerda + (direita - esquerda) / 2;

    se (arr[meio] == valor) {
        retornar meio; // Valor encontrado, retorna o índice
    }

    se (arr[meio] < valor) {
        esquerda = meio + 1; // Descartar a metade esquerda
    } outro {
        direita = meio - 1; // Descartar a metade direita
    }
}

retornar -1; // Valor não encontrado
}

int main() { int n, valor;

cout << "Digite o número de elementos no vetor: ";
cin >> n;

int* arr = novo int[n]; // Cria um vetor dinamicamente

cout << "Digite os elementos do vetor (em ordem crescente): ";
para (int i = 0; i < n; ++i) {
    cin >> arr[i];
}

cout << "Digite o valor a ser buscado: ";
cin >> valor;

int resultado = buscaBinaria(arr, n, valor);

se (resultado != -1) {
    cout << "Valor " << valor << " encontrado no índice " << resultado << "." << fim;
} outro {
    cout << "Valor " << valor << " não encontrado no vetor." << fim;
}

excluir[] arr; // Libera a memória alocada para o vetor

retornar 0;
}

