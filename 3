PERGUNTA 16: #incluir usando namespace std;

bool isPalindrome(int num) { int original = num; int invertido = 0;

// Converte o número para positivo se para negativo
se (num < 0) {
    retornar falso;
}

// Inverta o número
enquanto (num > 0) {
    int digito = num % 10;
    invertido = invertido * 10 + dígito;
    num /= 10;
}

retornar original == invertido;
}

int principal() { int num;

cout << "Digite um número: ";
cin >> num;

se (isPalindrome(num)) {
    cout << num << "é um palíndromo." << fim;
} outro {
    cout << num << "não é um palíndromo." << fim;
}

retornar 0;
}

PERGUNTA 17: #incluir usando namespace std;

// Função para calcular o Máximo Divisor Comum (MDC) usando o Algoritmo de Euclides int calcularMDC(int a, int b) { while (b != 0) { int resto = a % b; uma =b; b = resto; } retornar um; }

int principal() { int num1, num2;

cout << "Digite o primeiro número: ";
cin >> num1;

cout << "Digite o segundo número: ";
cin >> num2;

se (num1 < 0 || num2 < 0) {
    cout << "Os números devem ser não-negativos." << fim;
    retornar 1;
}

int mdc = calcularMDC(num1, num2);

cout << "O Máximo Divisor Comum (MDC) de " << num1 << " e " << num2 << " é: " << mdc << endl;

retornar 0;
}

PERGUNTA 18: #incluir usando namespace std;

// Função para calcular o Máximo Divisor Comum (MDC) usando o Algoritmo de Euclides int calcularMDC(int a, int b) { while (b != 0) { int resto = a % b; uma =b; b = resto; } retornar um; }

// Função para calcular o Mínimo Múltiplo Comum (MMC) int calcularMMC(int a, int b) { if (a == 0 || b == 0) return 0; // MMC de qualquer número com 0 é 0 return (a * b) / calcularMDC(a, b); }

int principal() { int num1, num2;

cout << "Digite o primeiro número: ";
cin >> num1;

cout << "Digite o segundo número: ";
cin >> num2;

se (num1 < 0 || num2 < 0) {
    cout << "Os números devem ser não-negativos." << fim;
    retornar 1;
}

int mmc = calcularMMC(num1, num2);

cout << "O Mínimo Múltiplo Comum (MMC) de " << num1 << " e " << num2 << " é: " << mmc << endl;

retornar 0;
}

PERGUNTA 19: #incluir usando namespace std;

// Função para verificar se um número é perfeito bool isPerfect(int num) { if (num <= 1) return false; // Números menores ou iguais a 1 não são perfeitos

int somaDivisores = 0;

//Calcula a soma dos divisores próprios
para (int i = 1; i <= num / 2; ++i) {
    se (num % i == 0) {
        somaDivisores += i;
    }
}

// Verifica se a soma dos divisores é igual ao número
retornar somaDivisores == num;
}

int principal() { int num;

cout << "Digite um número: ";
