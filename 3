PERGUNTA 11: #include using namespace std;

// Função para verificar se um número é primo bool isPrime(int num) { // Números menores ou iguais a 1 não são primos if (num <= 1) return false;

// 2 é o único número par que é primo
se (num == 2) retornar verdadeiro;

// Números pares maiores que 2 não são primos
se (num % 2 == 0) retornar falso;

// Verifica se o número é divisível por qualquer número ímpar
para (int i = 3; i * i <= num; i += 2) {
    se (num % i == 0) retornar falso;
}

retornar verdadeiro;
}

int principal() { int num;

cout << "Digite um número: ";
cin >> num;

se (isPrime(num)) {
    cout << num << "é um número primo." << fim;
} outro {
    cout << num << "não é um número primo." << fim;
}

retornar 0;
}

PERGUNTA 12: #incluir usando namespace std;

int principal() { int N;

cout << "Digite o valor de N: ";
cin >> N;

// Verifica se o valor de N é válido (não negativo)
se (N < 0) {
    cout << "O valor de N deve ser um número natural (não-negativo)." << fim;
    retornar 1;
}

//Calcula a soma usando a fórmula
int soma = N * (N + 1) / 2;

cout << "A soma dos primeiros " << N << " números naturais é: " << soma << endl;

retornar 0;
}

QUESTÃO 13: #include usando namespace std;

int principal() { int N;

cout << "Dígito o valor de N (número de termos): ";
cin >> N;

// Verifica se o valor de N é válido (positivo)
se (N <= 0) {
    cout << "O valor de N deve ser um número positivo." << fim;
    retornar 1;
}

// Variáveis ​​para armazenar os termos da sequência de Fibonacci
longo longo a = 0, b = 1, c;

cout << "Os primeiros " << N << " os termos da sequência de Fibonacci são:" << endl;

// Exibe o primeiro termo
se (N >= 1) cout << a << " ";

// Exibe o segundo termo
se (N >= 2) cout << b << " ";

// Calcula e exibe os seguintes termos
para (int i = 3; i <= N; ++i) {
    c = a + b;
    cout << c << " ";
    um = b;
    b = c;
}

cout << fim;

retornar 0;
}

PERGUNTA 14: #incluir usando namespace std;

int main() { int num, numInvertido = 0;

cout << "Digite um número inteiro: ";
cin >> num;

int numOriginal = num; // Armazene o número original para exibir depois, se necessário

// Converte o número para positivo se para negativo
se (num < 0) {
    num = -num;
}

// Inverta o número
enquanto (num > 0) {
    int dígito = num% 10; // Extrai o último dígito
    numInvertido = numInvertido * 10 + digito; // Adiciona o dígito ao novo número invertido
    num /= 10; //Remove o último dígito do número original
}

// Exibe o número invertido
cout << "Número invertido: " << (numOriginal < 0 ? -numInvertido : numInvertido) << endl;

retornar 0;
}

